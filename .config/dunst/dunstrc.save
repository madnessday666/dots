[global]
    font = JetBrains Mono 8
    allow_markup = yes
    format = "%s\n%b"
    sort = yes
    indicate_hidden = yes
    alignment = left
    bounce_freq = 0
    show_age_threshold = 60
    word_wrap = yes
    ignore_newline = no
    geometry = "300x10-10+60"
    ##transparency = 25
    transparency = 0
    idle_threshold = 120
    monitor = 0
    follow = mouse
    sticky_history = yes
    line_height = 5
    separator_height = 0
    padding = 5
    horizontal_padding = 5
    separator_color = #bfbfbf
    startup_notification = false
    show_indicators = no
    dmenu = /usr/bin/dmenu -p dunst
    browser = /usr/bin/google-chrome-beta -new-tab
    icon_position = left
    icon_folders = /home/vaughnvalle/.local/share/icons/ePapirus/24x24/panel:/home/vaughnvalle/.local/share/icons/ePapirus/apps:/home/vaughnvalle/.local/share/icons/ePapirus/devices:/home/vaughnvalle/.local/share/icons/ePapirus/actions

[frame]
    color = "#353e4b"
    width = 1

[shortcuts]
    close = ctrl+space
    close_all = ctrl+shift+space
    context = ctrl+shift+period 

[global]
    frame_color = "#1E66F5"
    separator_color= frame
    frame_width = 0

[urgency_low]
    background = "#EFF1F5"
    foreground = "#4C4F69"

[urgency_normal]
    background = "#EFF1F5"
    foreground = "#4C4F69"

[urgency_critical]
    background = "#EFF1F5"
    foreground = "#4C4F69"
    frame_color = "#FE640B"

[Messenger]
    appname = Discord Canary
    format = "<b>%s</b>\n%b"
    new_icon = Discord

[WMail]
    appname = Electron
    format = "<b>%s</b>\n%b"
    new_icon = WMail
    timeout = 2000

[NCMPCPP]
    appname = mpc
    format = "<b>Now Playing:</b>\n%s\n%b"
    #format = "<b>%s</b>\n%b"
    new_icon = NCMPCPP
    timeout = 10

[Spotify]
    appname = Spotify
    format = "<b>Now Playing:</b>\n%s\n%b"
    new_icon = /usr/share/app-info/icons/pop-artful-extra/64x64/spotify.png
    timeout = 10

[Transmission]
    appname = Transmission
    format = "<b>%s</b>\n%b"
    new_icon = Transmission
    timeout = 2000
